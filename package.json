{
  "name": "prisma-supabase-app",
  "version": "1.0.0",
  "description": "Node.js TypeScript app with Prisma, Supabase and Express",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "nodemon --exec ts-node src/index.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "prisma",
    "supabase",
    "postgresql",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "nodemon": "^3.0.0",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "cors": "^2.8.5",
    "@types/cors": "^2.8.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "helmet": "^7.0.0"
  }
}
